{
  "name": "lambdaconnect-js",
  "author": "Szymon Piechaczek",
  "browserslist": "> 0.25%, not dead",
  "version": "1.0.0",
  "main": "./lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "src",
    "es"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "babel src --out-dir es",
    "build:flow": "flow-copy-source -v src lib && flow-copy-source -v src es",
    "build": "npm run build:commonjs && npm run build:es && npm run build:flow",
    "clean": "rm -rf lib es",
    "prepare": "npm run clean && npm run build",
    "generate-model": "node script/generate-model.js",
    "test": "node node_modules/jest/bin/jest.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.17.8",
    "bluebird": "^3.7.2",
    "dexie": "^3.2.1",
    "dexie-observable": "^3.0.0-beta.11",
    "fast-xml-parser": "^3.21.1",
    "he": "^1.2.0",
    "isomorphic-fetch": "^3.0.0",
    "uuid": "^7.0.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-redux": "*",
    "redux": "*"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-flow": "^7.16.7",
    "@types/dexie": "^1.3.1",
    "@types/isomorphic-fetch": "0.0.36",
    "cross-env": "^7.0.3",
    "flow-bin": "^0.121.0",
    "flow-copy-source": "^2.0.9",
    "jest": "^28.1.1"
  }
}
